{
  "version": 2,
  "public": true,
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/register.html",
      "dest": "/register.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
